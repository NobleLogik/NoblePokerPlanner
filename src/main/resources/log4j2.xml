<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <File name="fileAppender" filename="logs/main.log" append="false">
      <PatternLayout>
        <Pattern>%d{HH:mm:ss.SSS} - %-5p - %C.%M - %m%n</Pattern>
      </PatternLayout>
    </File>
  </Appenders>
  <Loggers>
    <Root level="info">
      <AppenderRef ref="fileAppender"/>
    </Root>
  </Loggers>
</Configuration>
